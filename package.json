{
  "name": "react-es6-webpack-karma-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel) and Hot reloader using Webpack and Karma test runner.",
  "main": "index.js",
  "config": {
    "application": {
      "title": "Sample App"
    },
    "google": {
      "trackid": "UA-99999-XYZT"
    },
    "devserver": {
      "host": "localhost",
      "port": 5000
    }
  },
  "scripts": {
    "start": "node utils/server.js",
    "server": "npm run start",
    "assets": "node ./utils/template.js --input ./src/assets --output ./dist",
    "template": "node ./utils/template.js --input ./src/index.html --output ./dist/index.html",
    "modules": "node_modules/.bin/webpack --config webpack.config.production.js",
    "build": "npm run assets && npm run template && npm run modules",
    "test": "node_modules/.bin/karma start --single-run --no-auto-watch karma.config.js",
    "test_watch": "node_modules/.bin/karma start --auto-watch --no-single-run karma.config.js",
    "lint": "node_modules/.bin/eslint ./src ./test",
    "clean": "node_modules/.bin/rimraf build dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvader/react-es6-webpack-karma-boilerplate.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack",
    "karma"
  ],
  "author": {
    "name": "Miguel Molina",
    "email": "hi@mvader.me",
    "url": "http://mvader.me"
  },
  "contributors": [
    {
      "name": "Ionut Stoica",
      "email": "stoica.ionut@gmail.com",
      "url": "http://gion.ro"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvader/react-es6-webpack-karma-boilerplate/issues"
  },
  "homepage": "https://github.com/mvader/react-es6-webpack-karma-boilerplate",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "ejs": "^2.4.2",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-node": "^0.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.5.0",
    "mocha": "^2.5.3",
    "ncp": "^2.0.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "readline": "^1.3.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
